{
  "name": "ubitransport/coding-standards",
  "description": "Coding standards definition",
  "authors": [
    {
      "name": "Ubitransport",
      "homepage": "https://ubitransport.com/"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/ubitransports/php-code-sniffs.git"
    },
    {
      "type": "git",
      "url": "git@github.com:ubitransports/test-behat-extra-bundle.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ubitransport\\CodingStandards\\": "src/CodingStandards/",
      "Ubitransport\\Tests\\": "tests/"
    }
  },
  "require": {
    "friendsofphp/php-cs-fixer": "^v3.2.1",
    "infection/infection": "0.26.0",
    "insolita/unused-scanner": "^2.4",
    "phpstan/phpstan": "1.10.6",
    "phpstan/phpstan-doctrine": "1.3.35",
    "phpstan/phpstan-phpunit": "1.3.10",
    "phpstan/phpstan-symfony": "1.2.23",
    "phpunit/phpunit": "^9.5",
    "ubitransport/php-code-sniffs": "^4.5",
    "ubitransport/test-behat-extra-bundle": "^0.9.1",
    "wapmorgan/php-deprecation-detector": "^2.0"
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    },
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
